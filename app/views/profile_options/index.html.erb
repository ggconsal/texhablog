<p id="notice"><%= notice %></p>

<h1>Listing Profile Options</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Option</th>
      <th>Poc fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_options.each do |profile_option| %>
      <tr>
        <td><%= profile_option.profile %></td>
        <td><%= profile_option.option %></td>
        <td><%= profile_option.poc_fecha %></td>
        <td><%= link_to 'Show', profile_option %></td>
        <td><%= link_to 'Edit', edit_profile_option_path(profile_option) %></td>
        <td><%= link_to 'Destroy', profile_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile option', new_profile_option_path %>
